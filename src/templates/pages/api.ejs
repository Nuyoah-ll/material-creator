// 情况2：不区分请求前缀
const http = createHttp();

// 请求模板
// todo 在any处填写请求参数和响应参数
export function postRequest(params: any) {
  return http.post<unknown, any>("/postApi", params);
}

export function getRequest(params: any) {
  return http.get<unknown, any>("/getApi", {
    params,
  });
}
